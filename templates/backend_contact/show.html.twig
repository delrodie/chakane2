{% extends 'base.html.twig' %}

{% block title %}Contact{% endblock %}

{% block body %}
    <h1>Contact</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contact.id }}</td>
            </tr>
            <tr>
                <th>Tel1</th>
                <td>{{ contact.tel1 }}</td>
            </tr>
            <tr>
                <th>Tel2</th>
                <td>{{ contact.tel2 }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ contact.email }}</td>
            </tr>
            <tr>
                <th>Gmail</th>
                <td>{{ contact.gmail }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_backend_contact_index') }}">back to list</a>

    <a href="{{ path('app_backend_contact_edit', {'id': contact.id}) }}">edit</a>

    {{ include('backend_contact/_delete_form.html.twig') }}
{% endblock %}
